{
  "name": "@strmbrkr/plant-floor",
  "version": "0.1.0",
  "private": true,
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "android-dev": "ENVFILE=.env.dev react-native run-android --variant 'developmentDebug' --appId com.plantfloor.development",
    "android-prod": "ENVFILE=.env.prod react-native run-android --variant 'productionDebug' --appId com.plantfloor.production",
    "android-test": "ENVFILE=.env.test react-native run-android --variant 'stagingDebug' --appId com.plantfloor.test",
    "build": "heft test --clean",
    "ios-dev": "ENVFILE=.env.dev react-native run-ios --scheme 'Plant Floor - Development'",
    "ios-prod": "ENVFILE=.env.prod react-native run-ios --scheme 'Plant Floor - Production'",
    "ios-test": "ENVFILE=.env.test react-native run-ios --scheme 'Plant Floor - Test'",
    "start": "react-native webpack-start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4",
    "@react-native-community/cli-platform-android": "4.13.0",
    "@strmbrkr/components": "workspace:*",
    "@strmbrkr/framework": "workspace:*",
    "react-native-haptic-feedback": "1.11.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-config": "1.4.2",
    "@react-navigation/native": "5.9.3",
    "react-native-reanimated": "2.0.1",
    "react-native-gesture-handler": "1.10.3",
    "react-native-screens": "2.18.1",
    "react-native-safe-area-context": "3.2.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/stack": "5.14.3",
    "appcenter": "4.0.2",
    "appcenter-analytics": "4.0.2",
    "appcenter-crashes": "4.0.2",
    "react-native-webview": "11.2.5",
    "oidc-client": "1.11.5",
    "@react-native-async-storage/async-storage": "1.14.1",
    "react-native-app-auth": "~6.2.0"
  },
  "devDependencies": {
    "@strmbrkr/common-rig": "workspace:*",
    "@rushstack/heft": "0.25.1",
    "@strmbrkr/eslint-config": "workspace:*",
    "eslint": "7.22.0",
    "@react-native-community/eslint-config": "2.0.0",
    "@babel/core": "7.13.10",
    "@babel/runtime": "7.13.10",
    "@haul-bundler/babel-preset-react-native": "0.16.0",
    "@haul-bundler/cli": "0.22.1",
    "@haul-bundler/preset-0.60": "0.18.0",
    "@types/heft-jest": "1.0.2",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.35",
    "@storybook/react": "5.3.21",
    "@types/react": "16.8.0",
    "@types/react-native": "0.63.43",
    "react-test-renderer": "17.0.1",
    "@types/react-test-renderer": "17.0.1",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.65.2",
    "typescript": "4.2.3",
    "react-native-webpack-toolkit": "1.1.0",
    "webpack": "5.22.0",
    "terser-webpack-plugin": "~5.1.1",
    "babel-loader": "8.2.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
